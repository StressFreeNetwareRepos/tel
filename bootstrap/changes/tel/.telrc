#!/usr/bin/env bash

# # # # #  #
  #   #    #
  #   # #  #
  #   #    #
  #   # #  # # 

# Changes made here will not be permenant and is therefore advised users do not edit this file
. ~/.tel/scripts/readconfigs.sh
export PATH=/data/data/com.termux/files/usr/bin:/data/data/com.termux/files/usr/bin/applets:~/.tel/bin:/data/data/com.termux/files/home/.local/bin
set -K # disables ! events in zsh to allow for bangs in tel-search
source ~/.envvar # open this to see where to add your own environmental variables
source ~/.aliases # please open this file to see where to add you own aliases and functions
$HOME/.tel/scripts/readconfigs.sh # open this to find out about TEL settings

POWERLINE_BASH_CONTINUATION=1 # terminal prompt stuff
POWERLINE_BASH_SELECT=1 # terminal prompt stuff
powerline-daemon -q # start the prompt daemon
source /data/data/com.termux/files/usr/lib/python3.8/site-packages/powerline/bindings/zsh/powerline.zsh # prompt
export FZF_BASE=/data/data/com.termux/files/usr/bin/fzf
[ -f ~/.fzf.zsh ] && source ~/.fzf.zsh # load fzf bindings

PROMPT_EOL_MARK='' # removes the ugly % appended to partial lines in zsh



